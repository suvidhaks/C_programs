#include <stdio.h>

struct student
{
    int roll;
    float marks;
    char name[20];
}s[20];


int main()
{
    int n,i;
    printf("enter n");
    scanf("%d", &n);
    for(i=0;i<n;i++)
    {
        scanf("%d%f%s", &s[i].roll,&s[i].marks,&s[i].name);
    }
    
     for(i=0;i<n;i++)
    {
        printf("%d %0.2f %s", s[i].roll,s[i].marks,s[i].name);
        printf("\n");
    }

    return 0;
}



#include <stdio.h>
#include <stdlib.h>
struct node
{
    int data;
    struct node *link;
};
int main()
{
    
    struct node *head, *current;
    head=   (struct node * )  malloc(sizeof(struct node));
    head->data=45;
    head->link=NULL;
    printf("%d", head->data);
    current=(struct node *) malloc(sizeof(struct node));
    current->data =90;
    current->link=NULL;
    head->link=current;
     printf("\n%d", current->data);
    
    
    return 0;
}


#include <stdio.h>
#include<stdlib.h>
struct node
{
    int data;
   struct node *link;
};


int main()
{
   struct node *head, *curr, *next, *temp;
   head=(struct node *) malloc(sizeof(struct node));
   head->data=25;
   curr=(struct node *) malloc(sizeof(struct node));
   curr->data=50;
   head->link=curr;
   next=(struct node *) malloc(sizeof(struct node));
   next->data=75;
   curr->link=next;
   next->link=NULL;
   temp=head;
   while(temp!=NULL)
   {
       printf("\n%d", temp->data);
       temp=temp->link;
   }
   return 0;
}


/*insert node in the beginning*/


#include <stdio.h>
#include<stdlib.h>
struct node
{
    int data;
   struct node *link;
};


int main()
{
   struct node *head, *curr, *next, *temp;
   head=(struct node *) malloc(sizeof(struct node));
   head->data=25;
   curr=(struct node *) malloc(sizeof(struct node));
   curr->data=50;
   head->link=curr;
   next=(struct node *) malloc(sizeof(struct node));
   next->data=75;
   curr->link=next;
   next->link=NULL;
   temp=(struct node *) malloc(sizeof(struct node));
   temp->data=90;
   temp->link=head;
    head=temp;
   while(temp!=NULL)
   {
       printf("\n%d", temp->data);
       temp=temp->link;
   }
   return 0;
}
